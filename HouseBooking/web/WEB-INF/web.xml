<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AccountServlet</servlet-name>
        <servlet-class>Controller.ManageAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageHouseServlet</servlet-name>
        <servlet-class>Controller.ManageHouseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateHouseServlet</servlet-name>
        <servlet-class>Controller.UpdateHouseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NextEditHouseServlet</servlet-name>
        <servlet-class>Controller.NextEditHouseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteHouse</servlet-name>
        <servlet-class>Controller.DeleteHouseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NextAddHouseServlet</servlet-name>
        <servlet-class>Controller.NextAddHouseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddHouseServlet</servlet-name>
        <servlet-class>Controller.AddHouseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NextAddAccountServlet</servlet-name>
        <servlet-class>Controller.NextAddAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddAccountServlet</servlet-name>
        <servlet-class>Controller.AddAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NextEditAccountServlet</servlet-name>
        <servlet-class>Controller.NextEditAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateAccountServlet</servlet-name>
        <servlet-class>Controller.UpdateAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAccountServlet</servlet-name>
        <servlet-class>Controller.DeleteAccountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccountServlet</servlet-name>
        <url-pattern>/account</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageHouseServlet</servlet-name>
        <url-pattern>/ManageHouseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateHouseServlet</servlet-name>
        <url-pattern>/UpdateHouseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NextEditHouseServlet</servlet-name>
        <url-pattern>/next-edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteHouse</servlet-name>
        <url-pattern>/DeleteHouse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NextAddHouseServlet</servlet-name>
        <url-pattern>/next-add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddHouseServlet</servlet-name>
        <url-pattern>/addhouse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NextAddAccountServlet</servlet-name>
        <url-pattern>/next-add-account</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddAccountServlet</servlet-name>
        <url-pattern>/addaccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NextEditAccountServlet</servlet-name>
        <url-pattern>/next-edit-account</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateAccountServlet</servlet-name>
        <url-pattern>/editaccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAccountServlet</servlet-name>
        <url-pattern>/DeleteAccount</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
